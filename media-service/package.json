{
  "name": "@postxagent/media-service",
  "version": "2.0.0",
  "description": "AI Video & Music Generation Microservice for PostXAgent",
  "main": "dist/app.js",
  "author": "PostXAgent Development Team",
  "license": "PROPRIETARY",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "start:cluster": "pm2 start ecosystem.config.js --env production -i max",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "logs": "pm2 logs",
    "monitor": "pm2 monit",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "docker:build": "docker build -t postxagent/media-service:latest .",
    "docker:dev": "docker-compose -f docker/docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker/docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.1",
    "@prisma/client": "^5.8.0",
    "axios": "^1.6.5",
    "bullmq": "^5.1.8",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "fluent-ffmpeg": "^2.1.2",
    "ioredis": "^5.3.2",
    "mime-types": "^2.1.35",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.40.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.11",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "video-generation",
    "music-generation",
    "media-processing",
    "ffmpeg",
    "automation",
    "microservice",
    "typescript",
    "fastify",
    "bullmq",
    "playwright"
  ]
}
